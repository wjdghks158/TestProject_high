<?xml version="1.0" encoding="utf-8"?>
<com.example.unclej.testproject.customview.WrappedDrawerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    android:splitMotionEvents="false"
    tools:context="com.example.unclej.testproject.MainActivity">
    <FrameLayout android:id="@+id/content" android:layout_width="match_parent" android:layout_height="match_parent" />
    <android.support.design.widget.NavigationView android:layout_gravity="start" android:id="@+id/nav_left" android:fitsSystemWindows="true" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginLeft="5dp">
        <include layout="@layout/drawer_left_container" />
    </android.support.design.widget.NavigationView>
    <android.support.design.widget.NavigationView android:layout_gravity="end" android:id="@+id/nav_right" android:fitsSystemWindows="true" android:layout_width="match_parent" android:layout_height="match_parent" android:layout_marginLeft="5dp">
        <include layout="@layout/drawer_right_container" />
    </android.support.design.widget.NavigationView>




</com.example.unclej.testproject.customview.WrappedDrawerLayout>
